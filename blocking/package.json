{
  "name": "blocking-demo",
  "version": "1.0.0",
  "description": "Demonstrates blocking operations and event loop bottlenecks",
  "main": "src/index.js",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "start": "node src/index.js",
    "test": "node --test test/**/*.test.js",
    "test:watch": "node --test --watch test/**/*.test.js",
    "test:verbose": "node --test --reporter=spec test/**/*.test.js",
    "inspect": "node --inspect src/index.js",
    "inspect-brk": "node --inspect-brk src/index.js",
    "prof": "node --prof src/index.js",
    "clinic:doctor": "clinic doctor -- node src/index.js",
    "clinic:flame": "clinic flame -- node src/index.js",
    "0x": "0x -- node src/index.js",
    "load-test": "autocannon -c 10 -d 30 http://localhost:3000",
    "baseline": "autocannon -c 10 -d 30 -R -M http://localhost:3000",
    "compare": "autocannon -c 10 -d 30 -M http://localhost:3000"
  },
  "keywords": [
    "blocking",
    "event-loop",
    "performance"
  ],
